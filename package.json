{
  "name": "@fluidware-it/httpclient",
  "version": "0.2.1",
  "description": "http client package based on node fetch",
  "main": "build/src/index.js",
  "module": "build/esm/index.js",
  "esnext": "build/esnext/index.js",
  "types": "build/src/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "author": "Fluidware srl",
  "license": "Apache-2.0",
  "scripts": {
    "prepack": "npm run compile",
    "precompile": "tsc --version && npm run version:update",
    "version:update": "node ./scripts/version-update.js",
    "compile": "npm run clean && tsc --build tsconfig.json tsconfig.esm.json tsconfig.esnext.json",
    "clean": "tsc --build --clean tsconfig.json tsconfig.esm.json tsconfig.esnext.json",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "preversion": "npm run lint && npm run test",
    "pretest": "npm run compile",
    "test": "NODE_ENV=test c8 mocha --reporter mocha-multi-reporters tests/*.test.js",
    "start": "node src/index.ts",
    "coverage": "c8 report --reporter=cobertura --reporter=lcov"
  },
  "engines": {
    "node": ">=18.14"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/mocha": "^10.0.3",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "c8": "^8.0.1",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "express": "^4.18.2",
    "mocha": "^10.2.0",
    "mocha-junit-reporter": "^2.0.0",
    "mocha-multi-reporters": "^1.5.1",
    "mocha-sonarqube-reporter": "^1.0.2",
    "mocha-xunit-reporter": "^2.3.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "files": [
    "build/esm/**/*.js",
    "build/esm/**/*.js.map",
    "build/esm/**/*.d.ts",
    "build/esnext/**/*.js",
    "build/esnext/**/*.js.map",
    "build/esnext/**/*.d.ts",
    "build/src/**/*.js",
    "build/src/**/*.js.map",
    "build/src/**/*.d.ts",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "@fluidware-it/saddlebag": "^0.1.1"
  }
}
